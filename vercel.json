{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "assets/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "$1"
    },
    {
      "src": "/",
      "dest": "index.php"
    }
  ]
}
